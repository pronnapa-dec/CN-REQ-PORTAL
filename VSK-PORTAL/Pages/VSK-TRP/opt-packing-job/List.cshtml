                        <button type="button" class="btn btn-primary d-none" id="btn-add" style="width: 120px;">บันทึก</button>
                        <button type="button" class="btn btn-warning d-none" id="btn-edit" style="width: 120px;">แก้ไข</button>
                        @*<button type="reset" class="btn btn-danger" onclick="window.location.reload();">ยกเลิก</button>*@
                        <button type="button" class="btn btn-danger" id="btn-main" style="width: 120px;">ยกเลิก</button>
                    </div>
