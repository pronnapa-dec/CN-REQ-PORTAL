                        <label for="delivery_address" class="form-label">ที่อยู่จัดส่ง</label>
                        <select class="form-control select2" id="delivery_address" name="delivery_address" required="">
                            <option value="">--SELECT--</option>
                        </select>
                    </div>
                </div>

                <div class="row row-sm">
                    <div class="form-group col-3">
                        <label for="transport_start" class="form-label">ผู้ส่ง</label>
                        <select class="form-control select2" id="transport_start" name="transport_start" required="">
                            <option value="">--SELECT--</option>
                            <option value="KLH">KLH</option>
                            <option value="VSK" selected="">VSK</option>
                            <option value="VSF">VSF</option>
                            <option value="LLK">LLK</option>
                            <option value="NWM">NWM</option>
