                            <option value="CPS">CPS</option>
                            <option value="ONL">ONL</option>
                            <option value="MW">MW</option>
                        </select>
                    </div>
                    <div class="form-group col-3">
                        <label for="shipping_company" class="form-label">บริษัทขนส่ง</label>
                        <select class="form-control select2" id="shipping_company" name="shipping_company" required="">
                            <option value="" selected="">--SELECT--</option>
                        </select>
                    </div>
                    <div class="form-group col-3">
                        <label for="freight" class="form-label">ชำระค่าขนส่ง</label>
                        <select class="form-control select2" id="freight" name="freight" required="" style="width: 100%;">
                            <option value="">--SELECT--</option>
                            <option value="DC01" selected="">ต้นทาง</option>
                            <option value="DC02">ปลายทาง</option>
                        </select>
                    </div>
                    <div class="form-group col-3">
