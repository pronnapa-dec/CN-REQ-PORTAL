@page "/hr/time"

@{
    ViewData["Script"] = "apps/hr/hr-time.js";

    ViewData["Title"] = "Index";
    ViewData["Content-Title"] = "HR";
    ViewData["Content-Title-Page"] = "ข้อมูลเวลาพนักงาน";

}

@* container opened *@
<div class="container">

    @* subheader *@
    <div class="breadcrumb-header justify-content-between">

        @* breadcrumb *@
        <div class="my-auto">
            <div class="d-flex">
                <h4 class="content-title mb-0 my-auto">@ViewData["Content-Title"]</h4><span class="text-muted mt-1 tx-13 ml-2 mb-0">/ @ViewData["Content-Title-Page"]</span>
            </div>
        </div>
        @* breadcrumb *@

    </div>
    @* subheader *@

    @* datatables *@
    <div id="card-datatables" class="row">
        <partial name="~/Pages/VSK-HR/hr-time/Search.cshtml" />
        <partial name="~/Pages/VSK-HR/hr-time/List.cshtml" />

    </div>

    @* datatables *@
    <div id="card-datatables" class="row">
    </div>
    @* datatables *@

    @* modal form *@
    <div id="card-form" class="row">
        <partial name="~/Pages/VSK-HR/hr-time/Form.cshtml" />
    </div>
    @* modal form *@


</div>
@* Container closed *@
