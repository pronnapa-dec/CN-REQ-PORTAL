        <div class="modal-dialog modal-dialog-scrollable" role="document">
            <div class="modal-content modal-content-demo">
                <div class="modal-header">
                    <h6 class="modal-title">ประวัติเงินคูปอง</h6><button aria-label="Close" class="close" data-dismiss="modal" type="button"><span aria-hidden="true">&times;</span></button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-lg-12">
                            <table id="tbl-history_coupons" class="table table-bordered table-striped table-hover mg-b-0 text-md-nowrap" style="width:100%">
                                <thead style="font-size:11px; text-align:center">
                                    <tr>
                                        <th valign="middle" @*width="5%"*@ class="tx-center">ลำดับ</th>
                                        <th valign="middle" @*width="5%"*@ class="tx-center">วันที่/เวลา</th>
                                        <th valign="middle" @*width="40%"*@ class="tx-center">จำนวนเงิน</th>
